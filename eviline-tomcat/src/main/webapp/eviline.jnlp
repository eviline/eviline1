<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+" codebase="http://www.eviline.org/" href="eviline.jnlp">
    <information>
        <title>EVILINE</title>
        <vendor>Robin Kirkman</vendor>
        <offline-allowed/>
    </information>
    <security>
        <all-permissions/>
   </security>
    <resources>
        <!-- Application Resources -->
        <j2se version="1.6+"
            href="http://java.sun.com/products/autodl/j2se" />
        <jar href="http://www.eviline.org/WEB-INF/lib/eviline.jar" main="true" />
        <jar href="http://www.eviline.org/WEB-INF/lib/eviline-core.jar" />
        <jar href="http://www.eviline.org/WEB-INF/lib/eviline-clojure.jar" />
        <jar href="http://www.eviline.org/WEB-INF/lib/clojure.jar" />

    </resources>
    <applet-desc 
         name="EVILINE"
         main-class="org.eviline.runner.MainApplet"
         width="550"
         height="550">
     </applet-desc>
     <update check="always"/>
</jnlp>  